	"tokenColors": [
		{
			"name": "Source",
			"scope": ["source", "text"],
			"settings": {
				"foreground": "#cac6d3c7"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#4D4A56",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.template",
				"string.quoted.other"
			],
			"settings": {
				"foreground": "#b1d36d"
			}
		},
		{
			"name": "String Escape",
			"scope": ["constant.character.escape", "constant.other.placeholder"],
			"settings": {
				"foreground": "#FFD866"
			}
		},
		{
			"name": "String Interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#78DCE8"
			}
		},
		{
			"name": "Regular Expression",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#FF6188",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary"
			],
			"settings": {
				"foreground": "#33b3cc"
			}
		},
		{
			"name": "Boolean & Null",
			"scope": [
				"constant.language",
				"constant.language.boolean",
				"constant.language.null",
				"constant.language.undefined"
			],
			"settings": {
				"foreground": "#33b3cc"
			}
		},
		{
			"name": "Keyword - if",
			"scope": ["keyword.control.conditional", "keyword.control.loop"],
			"settings": {
				// "foreground": "#f47e98",
				// "foreground": "#fccffc",
				"foreground": "#e3e1e8a8",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword - export",
			"scope": ["keyword.control.export"],
			"settings": {
				"foreground": "#91aac0",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword - 'return', 'function'",
			"scope": ["keyword.control.flow", "storage.type.function"],
			"settings": {
				"foreground": "#7d7a8b",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword - import",
			"scope": [
				// "keyword.control.import",
				// "storage.type.type.ts",
				"keyword.operator.expression.import.ts"
			],
			"settings": {
				// "foreground": "#f47e98",
				"foreground": "#d09bd2",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.logical",
				"keyword.operator.wordlike"
			],
			"settings": {
				// "foreground": "#f47e98",
				"foreground": "#afd1e9cf",
				"fontStyle": ""
			}
		},

		{
			"name": "Operator",
			"scope": ["keyword.operator", "punctuation.accessor"],
			"settings": {
				"foreground": "#e60063"
			}
		},
		{
			"name": "Operator Special",
			"scope": [
				"keyword.operator.assignment",
				"keyword.operator.arithmetic",
				"keyword.operator.comparison",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#e60063"
			}
		},
		{
			"name": "Storage Type",
			"scope": ["storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#829297",
				"fontStyle": ""
			}
		},
		{
			"name": "Storage Modifier",
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#e3e1e8e4",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation - Accessor",
			"scope": ["punctuation.accessor"],
			"settings": {
				"foreground": "#7a73cc"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "variable.other"],
			"settings": {
				"foreground": "#e3e1e8c7"
			}
		},
		{
			"name": "Variable Language",
			"scope": [
				"variable.language.this",
				"variable.language.super",
				"variable.language.arguments"
			],
			"settings": {
				"foreground": "#ffa114",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Parameter",
			"scope": [
				"variable.parameter",
				"meta.function-call.arguments",
				"meta.function-call.arguments keyword.operator.assignment.python",
				"meta.parameters.ts keyword.operator.type.annotation",
				"meta.function.parameter"
			],
			"settings": {
				"foreground": "#8e8db3",
				// "foreground": "#e60063",
				// "foreground": "#faa629",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Function Parameter - value",
			"scope": ["variable.parameter.function-call"],
			"settings": {
				"foreground": "#c3c1d3",
				// "foreground": "#e60063",
				// "foreground": "#faa629",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Function Name",
			"scope": [
				"entity.name.function",
				"meta.function-call.generic",
				"support.function"
			],
			"settings": {
				"foreground": "#6bffbfdb",
				// "foreground": "#ffb547",
				"fontStyle": ""
			}
		},
		{
			"name": "Function Name (Go)",
			"scope": ["entity.name.function.support.go"],
			"settings": {
				// "foreground": "#6bffbfdb",
				"foreground": "#6bffd3db",
				"fontStyle": ""
			}
		},
		{
			"name": "Class Name",
			"scope": [
				"entity.name.class",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#ffb547",
				"fontStyle": "bold underline"
			}
		},
		{
			"name": "Keyword - Class",
			"scope": ["storage.type.class"],
			"settings": {
				"foreground": "#829197",
				"fontStyle": ""
			}
		},
		{
			"name": "JSX - Class",
			"scope": ["support.class.component.tsx", "support.class.component.jsx"],
			"settings": {
				"foreground": "#faa629",
				"fontStyle": ""
			}
		},
		{
			"name": "Type",
			"scope": ["entity.name.type", "support.type", "storage.type.primitive"],
			"settings": {
				"foreground": "#ffb547",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag Name",
			"scope": ["entity.name.tag", "support.class.component"],
			"settings": {
				"foreground": "#DEDEDE"
			}
		},
		{
			"name": "Tag Attribute",
			"scope": ["entity.other.attribute-name", "meta.tag.attributes"],
			"settings": {
				"foreground": "#FF7859"
			}
		},
		{
			"name": "CSS Property",
